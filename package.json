{
  "name": "web-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run generate && next dev",
    "build": "pnpm run generate && next build",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "generate": "pnpm run generate-old-api-client && pnpm run generate-new-api-client",
    "generate-old-api-client": "npx openapi-typescript https://staging.api.now-u.com/api-docs/v1/swagger.yaml --output src/services/causesApiTypes.ts",
    "generate-new-api-client": "npx openapi-typescript https://causes.dev.apiv2.now-u.com/api/schema/ --output src/services/apiTypes.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@tailwindcss/typography": "^0.5.10",
    "@trpc/client": "^10.41.0",
    "@trpc/next": "^10.41.0",
    "@trpc/server": "^10.41.0",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "axios": "^1.5.1",
    "client-only": "0.0.1",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.2",
    "next": "13.5.6",
    "nextjs-routes": "^2.0.1",
    "openapi-typescript-fetch": "^1.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-mobile-store-button": "^0.0.6",
    "typescript": "5.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/gtag.js": "0.0.17",
    "@types/mailchimp__mailchimp_marketing": "^3.0.16",
    "@types/markdown-it": "^13.0.4",
    "@types/node": "20.8.7",
    "autoprefixer": "^10.4.16",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3"
  }
}
